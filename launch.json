{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NestJS",
      "args": [
        "${workspaceFolder}/src/main.ts"
      ],
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "port": 5000,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src",
      "protocol": "inspector",
      "console": "integratedTerminal"
    }
  ]
}